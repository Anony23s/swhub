function xpOrb()
  gg.setVisible(false)
  gg.clearResults()
  
  -- Step 1: Find and temporarily edit Allowed bytes
  gg.searchNumber(":Allowed", gg.TYPE_BYTE)
  local revert = gg.getResults(100000)
  gg.editAll("0", gg.TYPE_BYTE)
  gg.clearResults()
  
  -- Step 2: Search the orb slash bundle
  gg.searchNumber(":*bundle_orbs_slash_4_2", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()
  
  -- Step 3: Get result and shift address by 0x378
  local t = gg.getResults(1)
  if #t == 0 then
    gg.alert("Nothing found.")
    return
  end

  for i, v in ipairs(t) do
    v.address = v.address + 0x378
    v.flags = gg.TYPE_BYTE
    v.value = "1"
    v.freeze = true
    v.freezeType = gg.FREEZE_NORMAL
  end

  gg.clearResults()
  gg.setValues(t)         -- Apply value = 1
  gg.addListItems(t)      -- Save to list
  gg.processResume()
gg.timeJump("5:0")
gg.toast("Done!")
  t = nil
end

function JackpotBox()
    gg.setVisible(false)
