function BasicOrbs()
  gg.setVisible(false)
  gg.clearResults()

  -- Step 1: Find and temporarily edit ":Allowed" bytes
  gg.searchNumber(":Allowed", gg.TYPE_BYTE)
  local r = gg.getResults(100000)
  gg.editAll("0", gg.TYPE_BYTE)
  gg.clearResults()

  -- Step 2: Find the basic orb bundle
  gg.searchNumber(":(bundle_orbs_basic_06", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()

  -- Step 3: Get result and shift address back by 0x38
  local t = gg.getResults(1)
  if #t == 0 then
    gg.alert("Nothing found.")
    return
  end

  for i, v in ipairs(t) do
    v.address = v.address - 0x38  -- 0xFFFFFFFFFFFFFFC8 is -0x38
    v.flags = gg.TYPE_BYTE
    v.value = "1"
    v.freeze = true
    v.freezeType = gg.FREEZE_NORMAL
  end

  gg.setValues(t)        -- Apply the value
  gg.addListItems(t)     -- Save to list
gg.clearResults()
  gg.processResume()
  gg.timeJump("5:0")
gg.toast("Done!")
end

function Bundle6Orbs()
  gg.setVisible(false)
  gg.clearResults()

  -- Step 1: Temporarily edit ":Allowed" values
  gg.searchNumber(":Allowed", gg.TYPE_BYTE)
  local r = gg.getResults(100000)
  gg.editAll("0", gg.TYPE_BYTE)
  gg.clearResults()

  -- Step 2: Search for the orb bundle pattern
  gg.searchNumber(":&bundle_orbs_core_06", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 1)
  gg.processResume()

  local t = gg.getResults(1)
  if #t == 0 then
    gg.alert("Nothing found.")
    return
  end

  -- Step 3: Adjust address, set value, and save
  for i, v in ipairs(t) do
    v.address = v.address - 0x38
    v.flags = gg.TYPE_BYTE
    v.value = "1"
    v.freeze = true
    v.freezeType = gg.FREEZE_NORMAL
  end

  gg.setValues(t)
  gg.addListItems(t)
gg.clearResults()
  gg.processResume()
  gg.timeJump("5:0")
gg.toast("Done!")
end

function Buranka_Britany()
  gg.setVisible(false)
  gg.clearResults()

  -- Step 1: Temporarily override :Allowed to 0
  gg.searchNumber(":Allowed", gg.TYPE_BYTE)
  local r = gg.getResults(100000)
  gg.editAll("0", gg.TYPE_BYTE)
  gg.clearResults()

  -- Step 2: Search for the target fire bundle
  gg.searchNumber(":&Bundle_Mutants_Fire", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()

  local t = gg.getResults(1)
  if #t == 0 then
    gg.alert("Nothing found.")
    return
  end

  -- Step 3: Offset by +0x1B8 and set value to 1
  for i, v in ipairs(t) do
    v.address = v.address + 0x1B8
    v.flags = gg.TYPE_BYTE
    v.value = "1"
    v.freeze = true
    v.freezeType = gg.FREEZE_NORMAL
  end

  gg.setValues(t)
  gg.addListItems(t)
gg.clearResults()
  gg.processResume()
  gg.timeJump("5:0")
gg.toast("Done!")
end

function MutantReactor()
  gg.setVisible(false)
  gg.clearResults()

  if not reactorReady then
    gg.alert("Reactor is not ready yet.")
    return
  end

  -- Replace main reactor hex
  gg.searchNumber(originalHex, gg.TYPE_BYTE)
  local r = gg.getResults(100000)
  gg.editAll(newHex, gg.TYPE_BYTE)
  gg.clearResults()

  -- Replace specimens
  for i = 1, #originalSpecimens do
    gg.searchNumber(":" .. originalSpecimens[i], gg.TYPE_BYTE)
    local s = gg.getResults(100000)
    gg.editAll(":" .. newSpecimens[i], gg.TYPE_BYTE)
    gg.clearResults()
  end

  gg.toast("Reactor Changed!")
  originalHex = nil
newHex = nil
originalSpecimens = nil
newSpecimens = nil
reactorReady = false
end

function ReadyFight()
    gg.setVisible(false)
    gg.clearResults()
    gg.searchNumber(":ready_fight", gg.TYPE_BYTE)
    local results = gg.getResults(100000)

    if #results == 0 then
        return
    end

    for i = 1, #results do
        results[i].value = 0
        results[i].freeze = true
    end

    gg.addListItems(results)
    gg.clearResults()
    gg.toast("Done!")
end

